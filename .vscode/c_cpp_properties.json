{
    "configurations": [
        {
            "name": "i686-w64-mingw",
            "includePath": [
                "/usr/i686-w64-mingw32/include/",
                "${workspaceFolder}/**",
                "${workspaceFolder}/src/ringworld/src/**"
            ],
            "compilerPath": "/usr/sbin/i686-w64-mingw32-gcc",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "windows-gcc-x86",
            "defines": ["RINGWORLD_ENABLE_ENHANCEMENTS", "DONT_FUCK_WITH_INTELLISENSE", "RINGWORLD_ENABLE_EVENTS_BUS"]
        }
    ],
    "version": 4
}
